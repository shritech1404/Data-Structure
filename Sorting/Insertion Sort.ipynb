   "source": [
    "def insertionSort(lst):\n",
    "  for i in range(1, len(lst)):\n",
    "    current_element = lst[i]\n",
    "    pos = i\n",
    "    while current_element<lst[pos-1] and pos>0:\n",
    "      lst[pos] = lst[pos-1]\n",
    "      pos -= 1\n",
    "    lst[pos] = current_element\n",
    "\n",
    "lst = [3,5,2,6,7,23,56,45,24,65,0]\n",
    "insertionSort(lst)\n",
    "print(lst)"
   ]
